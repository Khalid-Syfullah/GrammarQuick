<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="20dp"
    android:paddingLeft="20dp"
    android:paddingRight="20dp"
    tools:context=".Result">

    <android.support.v4.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">





            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="20dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:textStyle="bold"
                    android:text="Sentence"
                    android:textSize="20sp"/>
                <TextView
                    android:id="@+id/sentenceScore"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:layout_alignParentRight="true"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    />


                <TextView
                    android:layout_marginTop="20dp"
                android:layout_below="@id/sentenceScore"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:textStyle="bold"
                android:text="Parts Of Speech"
                android:textSize="20sp"/>
                <TextView
                    android:layout_marginTop="20dp"
                    android:id="@+id/partofspeechScore"
                    android:layout_below="@id/sentenceScore"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:layout_alignParentRight="true"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    />

                <TextView
                    android:layout_marginTop="20dp"
                    android:layout_below="@id/partofspeechScore"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:textStyle="bold"
                    android:text="Number"
                    android:textSize="20sp"/>
                <TextView
                    android:layout_marginTop="20dp"
                    android:id="@+id/numberScore"
                    android:layout_below="@id/partofspeechScore"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:layout_alignParentRight="true"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    />

                <TextView
                    android:layout_marginTop="20dp"
                    android:layout_below="@id/numberScore"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:textStyle="bold"
                    android:text="Gender"
                    android:textSize="20sp"/>
                <TextView
                    android:layout_marginTop="20dp"
                    android:id="@+id/genderScore"
                    android:layout_below="@id/numberScore"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:layout_alignParentRight="true"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    />
                <TextView
                    android:layout_marginTop="20dp"
                    android:layout_below="@id/genderScore"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:textStyle="bold"
                    android:text="Tense"
                    android:textSize="20sp"/>
                <TextView
                    android:layout_marginTop="20dp"
                    android:id="@+id/tenseScore"
                    android:layout_below="@id/genderScore"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:layout_alignParentRight="true"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    />
                <TextView
                    android:layout_marginTop="20dp"
                    android:layout_below="@id/tenseScore"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:textStyle="bold"
                    android:text="Article"
                    android:textSize="20sp"/>
                <TextView
                    android:layout_marginTop="20dp"
                    android:id="@+id/articleScore"
                    android:layout_below="@id/tenseScore"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:layout_alignParentRight="true"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    />

                <TextView
                    android:layout_marginTop="20dp"
                    android:layout_below="@id/articleScore"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:textStyle="bold"
                    android:text="Degree"
                    android:textSize="20sp"/>
                <TextView
                    android:layout_marginTop="20dp"
                    android:id="@+id/degreeScore"
                    android:layout_below="@id/articleScore"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:layout_alignParentRight="true"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    />
                <TextView
                    android:layout_marginTop="20dp"
                    android:layout_below="@id/degreeScore"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:textStyle="bold"
                    android:text="Voice"
                    android:textSize="20sp"/>
                <TextView
                    android:layout_marginTop="20dp"
                    android:id="@+id/voiceScore"
                    android:layout_below="@id/degreeScore"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:layout_alignParentRight="true"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    />
                <TextView
                    android:layout_marginTop="20dp"
                    android:layout_below="@id/voiceScore"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:textStyle="bold"
                    android:text="Transformation"
                    android:textSize="20sp"/>
                <TextView
                    android:layout_marginTop="20dp"
                    android:id="@+id/transformationScore"
                    android:layout_below="@id/voiceScore"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:layout_alignParentRight="true"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    />
                <Button
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/homeB"
                    android:text="Home"
                    android:background="@drawable/buttonrad"
                    android:layout_below="@+id/transformationScore"
                    android:layout_marginTop="50dp"
                    android:layout_centerHorizontal="true"/>
            </RelativeLayout>


    </android.support.v4.widget.NestedScrollView>

</LinearLayout>